@model TestViewMode

@{
    ViewData["Title"] = "测试类别";
}

<div class="panel panel-default todo-panel">
    <div class="panel-heading">@ViewData["Title"]</div>

    <table class="table table-hover">
        <thead>
            <tr>
                <td> <input type="checkbox" class="done-checkbox"></td>
                <td>姓名</td>
                <td>性别</td>
                <td>登陆名</td>
                <td>办公电话</td>
            </tr>
        </thead>

        @foreach (var item in Model.Tests)
        {
            <tr>
                <td><input type="checkbox" class="done-checkbox"></td>
                <td>@item.姓名</td>
                <td>@item.性别</td>
                <td>@item.登陆名</td>
                <td>@item.办公电话</td>
            </tr>
        }

    </table>
</div>
